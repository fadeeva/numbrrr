      "source": [
        "alph = 'АБВГДЕЖЗИЙКЛМНОПРСТУФХЦЧШЩЪЫЬЭЮЯ'\n",
        "N = len(alph)\n",
        "\n",
        "enc = 'ФРЗОРРТЯЫРЗГТЭЕХКФЯБЮФМЫОМОЯЬ'\n",
        "gamma = 'РАЗДРАКОНДВАДРАКОНАТРИДРАКОНА'"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "decrypted = []\n",
        "for e, g in zip(enc, gamma):\n",
        "    decrypted.append(alph[(alph.index(e)-alph.index(g))%N])\n",
        "\n",
        "print(*decrypted, sep='')"
      ],
